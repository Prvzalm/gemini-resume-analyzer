{
  "version": 2,
  "builds": [
    { "src": "apps/web/package.json", "use": "@vercel/next" },
    {
      "src": "apps/api/api/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["apps/api/src/**/*"] }
    }
  ],
  "routes": [{ "src": "/api/(.*)", "dest": "/apps/api/api/index.ts" }]
}
