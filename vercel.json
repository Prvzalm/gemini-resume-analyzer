{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "apps/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "apps/api/dist/**",
          "apps/web/.next/**",
          "apps/web/public/**",
          "packages/**"
        ]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/apps/api/index" }]
}
