{
  "name": "resume-analyzer",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered resume and job match SaaS platform",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:web dev:api",
    "dev:web": "npm run dev --workspace apps/web",
    "dev:api": "npm run dev --workspace apps/api",
    "build": "npm --prefix packages/config run build && npm --prefix packages/core run build && npm --prefix packages/ui run build && npm --prefix apps/web run build && npm run copy:frontend && npm --prefix apps/api run build",
    "copy:frontend": "node scripts/copy-frontend.js",
    "start": "npm run start --workspace apps/api",
    "lint": "npm-run-all lint:web lint:api",
    "lint:web": "npm run lint --workspace apps/web",
    "lint:api": "npm run lint --workspace apps/api",
    "test": "npm run test --workspace packages/core"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "keywords": [
    "resume",
    "ai",
    "saas",
    "job-match"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
